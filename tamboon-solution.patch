+    ? window.__REDUX_DEVTOOLS_EXTENSION_COMPOSE__
+    : compose;
+
+export default function configureStore(initialState) {
+  const middlewares = [thunk];
+  if (!PRODUCTION) {
+    middlewares.push(logger);
+  }
+  return createStore(
